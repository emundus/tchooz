<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
>
    <title>phpDocumentor</title>

    <paths>
        <output>./components/com_emundus/docs/back</output>
    </paths>

    <version number="3.0.0">
        <api>
            <source dsn="./components/com_emundus">
                <path>controllers</path>
                <path>helpers</path>
                <path>models</path>
            </source>
        </api>
    </version>

    <!-- Specify template element inside phpdocumentor -->
    <template name="./libraries/emundus/vendor/saggre/phpdocumentor-markdown/themes/markdown"/>

</phpdocumentor>