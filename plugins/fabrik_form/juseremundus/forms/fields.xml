<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<form>
	<fields
		addfieldpath="/administrator/components/com_fabrik/models/fields"
		name="params">
		
		<fieldset name="plg-form-juseremundus" label="PLG_FABRIK_FORM_JUSEREMUNDUS_FIELDS">

			<field name="juseremundus_field_name"
				type="listfields"
				default="user"
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_NAME_FIELD_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_NAME_FIELD_LABEL"
				onlytablefields="0"
				repeat="true" />

			<field name="juseremundus_field_username"
				type="listfields"
				default="username"
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_USERNAME_FIELD_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_USERNAME_FIELD_LABEL"
				onlytablefields="0"
				repeat="true" />

			<field name="juseremundus_field_password"
				type="listfields"
				default="password"
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_PASSWORD_FIELD_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_PASSWORD_FIELD_LABEL"
				onlytablefields="0"
				repeat="true" />

			<field name="juseremundus_field_email"
				type="listfields"
				default="email"
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_EMAIL_FIELD_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_EMAIL_FIELD_LABEL"
				onlytablefields="0"
				repeat="true" />

			<field name="juseremundus_field_block"
				type="listfields"
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_BLOCK_FIELD_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_BLOCK_FIELD_LABEL"
				onlytablefields="0"
				repeat="true" />

			<field name="juseremundus_field_userid"
				type="listfields"
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_USER_ID_FIELD_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_USER_ID_FIELD_LABEL"
				onlytablefields="0"
				repeat="true" />


			<field name="juseremundus_field_usertype"
				type="listfields"
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_USERTYPE_FIELD_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_USERTYPE_FIELD_LABEL"
				onlytablefields="0"
				repeat="true" />

		</fieldset>
		
		<fieldset name="plg-form-juseremundus-bypass" label="PLG_FABRIK_FORM_JUSEREMUNDUS_BYPASS" description="text">

			<field name="juseremundus_bypass_activation"
				type="radio"
				default="0"
				class="btn-group"
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_BYPASS_ACTIVATION_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_BYPASS_ACTIVATION_LABEL"
				repeat="true">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
			</field>

			<field name="juseremundus_bypass_registration"
				type="radio"
				default="1"
				class="btn-group"
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_BYPASS_REGISTRATION_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_BYPASS_REGISTRATION_LABEL"
				repeat="true">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
			</field>

			<field name="juseremundus_bypass_accountdetails"
				type="radio"
				default="0"
				class="btn-group"
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_BYPASS_J_ACCOUNT_DETAILS_EMAIL_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_BYPASS_J_ACCOUNT_DETAILS_EMAIL_LABEL"
				repeat="true">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
			</field>
			
			<field name="juseremundus_use_email_plugin"
				type="radio"
				default="0"
				class="btn-group"
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_USE_EMAIL_PLUGIN_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_USE_EMAIL_PLUGIN_LABEL"
				repeat="true">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
			</field>
			
		</fieldset>
		
		<fieldset name="plg-form-juseremundus-options" label="COM_FABRIK_OPTIONS">

			<field name="juseremundus_field_default_group"
				type="usergroup"
				default="0"
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_DEFAULT_USERTYPE_FIELD_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_DEFAULT_USERTYPE_FIELD_LABEL"
				repeat="true" />

			<field name="juseremundus_conditon"
				type="fabrikeditor"
				mode="php"
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_CONDITION_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_CONDITION_LABEL"
				repeat="true" />
		
			<field name="juseremundus_field_default_profile"
				type="field"
				default="0"
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_DEFAULT_PROFILE_FIELD_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_DEFAULT_PROFILE_FIELD_LABEL"
				repeat="true" />
			<field name="juseremundus_field_default_email"
				type="field"
				default=""
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_DEFAULT_EMAIL_FIELD_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_DEFAULT_EMAIL_FIELD_LABEL"
				repeat="true" />

		</fieldset>
		
		<fieldset name="plg-form-juseremundus-sycn" label="PLG_FABRIK_FORM_JUSEREMUNDUS_SYNC">

			<field name="juseremundus_delete_user"
				type="radio"
				default="0"
				class="btn-group"
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_DELETE_USERS_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_DELETE_USERS_LABEL"
				repeat="true">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
			</field>

			<field name="juseremundus_auto_login"
				type="radio"
				default="0"
				class="btn-group"
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_AUTO_LOGIN_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_AUTO_LOGIN_LABEL"
				repeat="true">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
			</field>

			<field name="synchro_users"
				type="radio"
				default="0"
				class="btn-group"
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_SYNCRONIZE_USERS_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_SYNCRONIZE_USERS_LABEL"
				repeat="true">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
			</field>

			<field name="juseremundus_sync_on_edit"
				type="radio"
				default="0"
				class="btn-group"
				description="PLG_FABRIK_FORM_JUSEREMUNDUS_JUSEREMUNDUSSYNCONEDIT_DESC"
				label="PLG_FABRIK_FORM_JUSEREMUNDUS_JUSEREMUNDUSSYNCONEDIT_LABEL"
				repeat="true">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
			</field>

		</fieldset>
	</fields>
</form>
